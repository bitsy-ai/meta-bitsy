[Unit]
Description=PrintNanny RTP Server
After=network-online.target printnanny-cloud-sync.service
Wants=network-online.target printnanny-cloud-sync.service 

[Service]
Type=simple
SyslogIdentifier=printnanny-cam
ExecStart=bash -c '/usr/bin/printnanny-gst-cam'
ExecStop= bash -c 'rm -f /var/run/printnanny/video-raw.socket'
Restart=on-failure
RestartSec=30
User=printnanny
[Install]
WantedBy=multi-user.target
