[Unit]
Description=PrintNanny RTP Server
After=network-online.target printnanny-cloud-sync.service
Wants=network-online.target printnanny-cloud-sync.service 

[Service]
Type=simple
SyslogIdentifier=printnanny-cam
ExecStart=/usr/local/bin/printnanny-gst-cam
Restart=on-failure
RestartSec=30
User=printnanny
[Install]
WantedBy=multi-user.target
