[Unit]
Description=PrintNanny Cam Service
After=network-online.target printnanny-cloud-sync.service
Wants=network-online.target printnanny-cloud-sync.service 

[Service]
Type=simple
SyslogIdentifier=printnanny-vision
EnvironmentFile=-/var/run/printnanny/printnanny-gst-vision.conf
ExecStart=/usr/bin/printnanny-gst-pipeline
Restart=on-failure
RestartSec=30
User=printnanny

[Install]
WantedBy=multi-user.target
